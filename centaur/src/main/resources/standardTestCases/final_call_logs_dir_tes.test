# Tests that a runtime option to save call logs is copying said logs appropriately.

name: final_call_logs_dir_tes
backends: [Tes]

testFormat: workflowsuccess

files {
  workflow: final_call_logs_dir/final_call_logs_dir.wdl
  options: final_call_logs_dir/final_call_logs_dir_local.options
}

fileSystemCheck: "local"
outputExpectations: {
  "/tmp/call_logs/wf_hello/<<UUID>>/call-hello/execution/attempt-1/stdout": 1
  "/tmp/call_logs/wf_hello/<<UUID>>/call-hello/execution/attempt-1/stderr": 1
  "/tmp/logs/workflow.<<UUID>>.log": 1
  "/tmp/outputs/wf_hello/<<UUID>>/call-hello/execution/attempt-1/test.out": 1
  "/tmp/outputs/wf_hello/<<UUID>>/call-hello/execution/attempt-1/array.out": 1
  "/tmp/outputs/wf_hello/<<UUID>>/call-hello/execution/attempt-1/map.out": 1
  "/tmp/outputs/wf_hello/<<UUID>>/call-hello/execution/attempt-1/left.out": 1
  "/tmp/outputs/wf_hello/<<UUID>>/call-hello/execution/attempt-1/right.out": 1
}
